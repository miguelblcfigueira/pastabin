{
  "env": {
    "browser": false,
    "commonjs": true,
    "es2021": true,
    "jest/globals": true
  },
  "extends": [
    "airbnb-base",
    "plugin:jest/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-underscore-dangle": [
      "off"
    ]
  },
  "plugins": [
    "jest"
  ]
}